<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:orientation="vertical">

            <include
                android:id="@+id/basicinfo_layout"
                layout="@layout/layout_basic_user_info" />

            <include
                android:id="@+id/idcard_layout"
                layout="@layout/layout_report_idcard" />

            <include
                android:id="@+id/job_layout"
                layout="@layout/layout_job_info" />

            <include
                android:id="@+id/contact_layout"
                layout="@layout/layout_report_contact" />

            <include
                android:id="@+id/zm_layout"
                layout="@layout/layout_report_zm" />

            <include
                android:id="@+id/location_layout"
                layout="@layout/layout_report_location" />

            <include
                android:id="@+id/address_book_layout"
                layout="@layout/layout_report_addressbok" />

            <include
                android:id="@+id/focus_on_layout"
                layout="@layout/layout_report_focus_on" />

            <include
                android:id="@+id/foraud_layout"
                layout="@layout/layout_report_fraud" />

            <include
                android:id="@+id/td_layout"
                layout="@layout/layout_report_td" />
        </LinearLayout>


    </ScrollView>
</layout>
